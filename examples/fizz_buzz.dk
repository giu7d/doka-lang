-- Input fizz_buzz.dk --

module FizzBuzz
	fun fizz_buzz(value):
		case:
			(value % 15 == 0) -> "FizzBuzz"
			(value % 5 == 0) -> "Fizz"
			(value % 3 == 0) -> "Buzz"
			_ -> Nil
        end
    end
	
	
-- Output fizz_buzz.js --

export default class FizzBuzz {

	static fizz_buzz(value) {
		if (value % 15 == 0) {
			return "FizzBuzz"
		}
		
		if (value % 5 == 0) {
			return "Fizz"
		}

		if (value % 3 == 0) {
			return "Buzz"
		}
		
		return null
	}	
}
